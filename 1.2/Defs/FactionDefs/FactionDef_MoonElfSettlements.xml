<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <FactionDef ParentName="O21_ElvenSettlementsBase">
        <defName>O21_MoonElfKingdoms</defName>
        <label>Moon elf settlements</label>
        <description>The collective settlements of Moon elves in this realm.</description>
        <factionIconPath>World/Elves/MoonElfIcon</factionIconPath>
        <colorSpectrum>
            <li>RGB(175, 223, 227)</li>
        </colorSpectrum>
        <startingGoodwill>
            <min>-0</min>
            <max>0</max>
        </startingGoodwill>
        <naturalColonyGoodwill>
            <min>-50</min>
            <max>50</max>
        </naturalColonyGoodwill>
        <pawnGroupMakers>
            <li>
                <!-- Normal fights, ranged with melee mix-ins -->
                <kindDef>Combat</kindDef>
                <commonality>100</commonality>
                <options>
                    <O21_ElvenSettlements_Peasant_Moon>20</O21_ElvenSettlements_Peasant_Moon>
                    <O21_ElvenSettlements_Squire_Moon>40</O21_ElvenSettlements_Squire_Moon>
                    <O21_ElvenSettlements_Footsoldier_Moon>30</O21_ElvenSettlements_Footsoldier_Moon>
                    <O21_ElvenSettlements_Bowman_Moon>30</O21_ElvenSettlements_Bowman_Moon>
                    <O21_ElvenSettlements_Knight_Moon>30</O21_ElvenSettlements_Knight_Moon>
                    <O21_ElvenSettlements_Lord_Moon>10</O21_ElvenSettlements_Lord_Moon>
                </options>
            </li>
            <li>
                <!-- Normal fights, ranged only -->
                <kindDef>Combat</kindDef>
                <commonality>60</commonality>
                <options>
                    <O21_ElvenSettlements_Bowman_Moon>100</O21_ElvenSettlements_Bowman_Moon>
                </options>
            </li>
            <li>
                <!-- Normal fights, melee only -->
                <kindDef>Combat</kindDef>
                <commonality>60</commonality>
                <options>
                    <O21_ElvenSettlements_Peasant_Moon>15</O21_ElvenSettlements_Peasant_Moon>
                    <O21_ElvenSettlements_Squire_Moon>40</O21_ElvenSettlements_Squire_Moon>
                    <O21_ElvenSettlements_Footsoldier_Moon>40</O21_ElvenSettlements_Footsoldier_Moon>
                    <O21_ElvenSettlements_Knight_Moon>30</O21_ElvenSettlements_Knight_Moon>
                    <O21_ElvenSettlements_Lord_Moon>5</O21_ElvenSettlements_Lord_Moon>
                </options>
            </li>
            <li>
                <!-- Peaceful -->
                <kindDef>Peaceful</kindDef>
                <options>
                    <O21_ElvenSettlements_Squire_Moon>30</O21_ElvenSettlements_Squire_Moon>
                    <O21_ElvenSettlements_Footsoldier_Moon>30</O21_ElvenSettlements_Footsoldier_Moon>
                    <O21_ElvenSettlements_Bowman_Moon>30</O21_ElvenSettlements_Bowman_Moon>
                    <O21_ElvenSettlements_Knight_Moon>20</O21_ElvenSettlements_Knight_Moon>
                    <O21_ElvenSettlements_Lord_Moon>10</O21_ElvenSettlements_Lord_Moon>
                </options>
            </li>
            <li>
                <kindDef>Trader</kindDef>
                <traders>
                    <O21_ElvenSettlements_Merchant_Moon>1</O21_ElvenSettlements_Merchant_Moon>
                </traders>
                <carriers>
                    <Muffalo>6</Muffalo>
                    <Dromedary>5</Dromedary>
                    <Alpaca>2.5</Alpaca>
                    <Elephant>2</Elephant>
                </carriers>
                <guards>
                    <O21_ElvenSettlements_Squire_Moon>30</O21_ElvenSettlements_Squire_Moon>
                    <O21_ElvenSettlements_Footsoldier_Moon>30</O21_ElvenSettlements_Footsoldier_Moon>
                    <O21_ElvenSettlements_Bowman_Moon>30</O21_ElvenSettlements_Bowman_Moon>
                    <O21_ElvenSettlements_Knight_Moon>20</O21_ElvenSettlements_Knight_Moon>
                </guards>
            </li>
            <li>
                <kindDef>Settlement</kindDef>
                <options>
                    <O21_ElvenSettlements_Peasant_Moon>20</O21_ElvenSettlements_Peasant_Moon>
                    <O21_ElvenSettlements_Squire_Moon>20</O21_ElvenSettlements_Squire_Moon>
                    <O21_ElvenSettlements_Footsoldier_Moon>20</O21_ElvenSettlements_Footsoldier_Moon>
                    <O21_ElvenSettlements_Bowman_Moon>50</O21_ElvenSettlements_Bowman_Moon>
                    <O21_ElvenSettlements_Knight_Moon>30</O21_ElvenSettlements_Knight_Moon>
                    <O21_ElvenSettlements_Lord_Moon>50</O21_ElvenSettlements_Lord_Moon>
                </options>
            </li>
        </pawnGroupMakers>
    </FactionDef>

</Defs>