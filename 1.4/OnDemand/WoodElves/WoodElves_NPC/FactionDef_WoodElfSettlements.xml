<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <FactionDef ParentName="FRealms_ElvenSettlementsBase">
        <defName>FRealms_WoodElfKingdoms</defName>
        <label>Wood elf settlements</label>
        <description>The collective settlements of Wood elves in this realm.</description>
        <factionIconPath>World/Elves/WoodElfIcon</factionIconPath>
        <colorSpectrum>
            <li>RGB(185, 227, 175)</li>
        </colorSpectrum>
        <pawnGroupMakers>
            <li>
                <!-- Normal fights, ranged with melee mix-ins -->
                <kindDef>Combat</kindDef>
                <commonality>100</commonality>
                <options>
                    <FRealms_ElvenSettlements_Peasant_Wood>20</FRealms_ElvenSettlements_Peasant_Wood>
                    <FRealms_ElvenSettlements_Squire_Wood>40</FRealms_ElvenSettlements_Squire_Wood>
                    <FRealms_ElvenSettlements_Footsoldier_Wood>30</FRealms_ElvenSettlements_Footsoldier_Wood>
                    <FRealms_ElvenSettlements_Bowman_Wood>30</FRealms_ElvenSettlements_Bowman_Wood>
                    <FRealms_ElvenSettlements_Knight_Wood>30</FRealms_ElvenSettlements_Knight_Wood>
                    <FRealms_ElvenSettlements_Lord_Wood>10</FRealms_ElvenSettlements_Lord_Wood>
                </options>
            </li>
            <li>
                <!-- Normal fights, ranged only -->
                <kindDef>Combat</kindDef>
                <commonality>60</commonality>
                <options>
                    <FRealms_ElvenSettlements_Bowman_Wood>100</FRealms_ElvenSettlements_Bowman_Wood>
                </options>
            </li>
            <li>
                <!-- Normal fights, melee only -->
                <kindDef>Combat</kindDef>
                <commonality>60</commonality>
                <options>
                    <FRealms_ElvenSettlements_Peasant_Wood>15</FRealms_ElvenSettlements_Peasant_Wood>
                    <FRealms_ElvenSettlements_Squire_Wood>40</FRealms_ElvenSettlements_Squire_Wood>
                    <FRealms_ElvenSettlements_Footsoldier_Wood>40</FRealms_ElvenSettlements_Footsoldier_Wood>
                    <FRealms_ElvenSettlements_Knight_Wood>30</FRealms_ElvenSettlements_Knight_Wood>
                    <FRealms_ElvenSettlements_Lord_Wood>5</FRealms_ElvenSettlements_Lord_Wood>
                </options>
            </li>
            <li>
                <!-- Peaceful -->
                <kindDef>Peaceful</kindDef>
                <options>
                    <FRealms_ElvenSettlements_Squire_Wood>30</FRealms_ElvenSettlements_Squire_Wood>
                    <FRealms_ElvenSettlements_Footsoldier_Wood>30</FRealms_ElvenSettlements_Footsoldier_Wood>
                    <FRealms_ElvenSettlements_Bowman_Wood>30</FRealms_ElvenSettlements_Bowman_Wood>
                    <FRealms_ElvenSettlements_Knight_Wood>20</FRealms_ElvenSettlements_Knight_Wood>
                    <FRealms_ElvenSettlements_Lord_Wood>10</FRealms_ElvenSettlements_Lord_Wood>
                </options>
            </li>
            <li>
                <kindDef>Trader</kindDef>
                <traders>
                    <FRealms_ElvenSettlements_Merchant_Wood>1</FRealms_ElvenSettlements_Merchant_Wood>
                </traders>
                <carriers>
                    <Muffalo>6</Muffalo>
                    <Dromedary>5</Dromedary>
                    <Alpaca>2.5</Alpaca>
                    <Elephant>2</Elephant>
                </carriers>
                <guards>
                    <FRealms_ElvenSettlements_Squire_Wood>30</FRealms_ElvenSettlements_Squire_Wood>
                    <FRealms_ElvenSettlements_Footsoldier_Wood>30</FRealms_ElvenSettlements_Footsoldier_Wood>
                    <FRealms_ElvenSettlements_Bowman_Wood>30</FRealms_ElvenSettlements_Bowman_Wood>
                    <FRealms_ElvenSettlements_Knight_Wood>20</FRealms_ElvenSettlements_Knight_Wood>
                </guards>
            </li>
            <li>
                <kindDef>Settlement</kindDef>
                <options>
                    <FRealms_ElvenSettlements_Peasant_Wood>20</FRealms_ElvenSettlements_Peasant_Wood>
                    <FRealms_ElvenSettlements_Squire_Wood>20</FRealms_ElvenSettlements_Squire_Wood>
                    <FRealms_ElvenSettlements_Footsoldier_Wood>20</FRealms_ElvenSettlements_Footsoldier_Wood>
                    <FRealms_ElvenSettlements_Bowman_Wood>50</FRealms_ElvenSettlements_Bowman_Wood>
                    <FRealms_ElvenSettlements_Knight_Wood>30</FRealms_ElvenSettlements_Knight_Wood>
                    <FRealms_ElvenSettlements_Lord_Wood>50</FRealms_ElvenSettlements_Lord_Wood>
                </options>
            </li>
        </pawnGroupMakers>
    </FactionDef>

</Defs>