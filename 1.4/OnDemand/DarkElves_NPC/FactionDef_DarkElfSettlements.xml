<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <FactionDef ParentName="FRealms_ElvenSettlementsBase">
        <defName>FRealms_DarkElfKingdoms</defName>
        <label>drow settlements</label>
        <description>The collective settlements of Drow in this realm.</description>
        <factionIconPath>World/Elves/DarkElfIcon</factionIconPath>
        <colorSpectrum>
            <li>RGB(250, 250, 250)</li>
        </colorSpectrum>
        <pawnGroupMakers>
            <li>
                <!-- Normal fights, ranged with melee mix-ins -->
                <kindDef>Combat</kindDef>
                <commonality>100</commonality>
                <options>
                    <FRealms_ElvenSettlements_Peasant_Dark>20</FRealms_ElvenSettlements_Peasant_Dark>
                    <FRealms_ElvenSettlements_Squire_Dark>40</FRealms_ElvenSettlements_Squire_Dark>
                    <FRealms_ElvenSettlements_Footsoldier_Dark>30</FRealms_ElvenSettlements_Footsoldier_Dark>
                    <FRealms_ElvenSettlements_Bowman_Dark>30</FRealms_ElvenSettlements_Bowman_Dark>
                    <FRealms_ElvenSettlements_Knight_Dark>30</FRealms_ElvenSettlements_Knight_Dark>
                    <FRealms_ElvenSettlements_Lord_Dark>10</FRealms_ElvenSettlements_Lord_Dark>
                </options>
            </li>
            <li>
                <!-- Normal fights, ranged only -->
                <kindDef>Combat</kindDef>
                <commonality>60</commonality>
                <options>
                    <FRealms_ElvenSettlements_Bowman_Dark>100</FRealms_ElvenSettlements_Bowman_Dark>
                </options>
            </li>
            <li>
                <!-- Normal fights, melee only -->
                <kindDef>Combat</kindDef>
                <commonality>60</commonality>
                <options>
                    <FRealms_ElvenSettlements_Peasant_Dark>15</FRealms_ElvenSettlements_Peasant_Dark>
                    <FRealms_ElvenSettlements_Squire_Dark>40</FRealms_ElvenSettlements_Squire_Dark>
                    <FRealms_ElvenSettlements_Footsoldier_Dark>40</FRealms_ElvenSettlements_Footsoldier_Dark>
                    <FRealms_ElvenSettlements_Knight_Dark>30</FRealms_ElvenSettlements_Knight_Dark>
                    <FRealms_ElvenSettlements_Lord_Dark>5</FRealms_ElvenSettlements_Lord_Dark>
                </options>
            </li>
            <li>
                <!-- Peaceful -->
                <kindDef>Peaceful</kindDef>
                <options>
                    <FRealms_ElvenSettlements_Squire_Dark>30</FRealms_ElvenSettlements_Squire_Dark>
                    <FRealms_ElvenSettlements_Footsoldier_Dark>30</FRealms_ElvenSettlements_Footsoldier_Dark>
                    <FRealms_ElvenSettlements_Bowman_Dark>30</FRealms_ElvenSettlements_Bowman_Dark>
                    <FRealms_ElvenSettlements_Knight_Dark>20</FRealms_ElvenSettlements_Knight_Dark>
                    <FRealms_ElvenSettlements_Lord_Dark>10</FRealms_ElvenSettlements_Lord_Dark>
                </options>
            </li>
            <li>
                <kindDef>Trader</kindDef>
                <traders>
                    <FRealms_ElvenSettlements_Merchant_Dark>1</FRealms_ElvenSettlements_Merchant_Dark>
                </traders>
                <carriers>
                    <Muffalo>6</Muffalo>
                    <Dromedary>5</Dromedary>
                    <Alpaca>2.5</Alpaca>
                    <Elephant>2</Elephant>
                </carriers>
                <guards>
                    <FRealms_ElvenSettlements_Squire_Dark>30</FRealms_ElvenSettlements_Squire_Dark>
                    <FRealms_ElvenSettlements_Footsoldier_Dark>30</FRealms_ElvenSettlements_Footsoldier_Dark>
                    <FRealms_ElvenSettlements_Bowman_Dark>30</FRealms_ElvenSettlements_Bowman_Dark>
                    <FRealms_ElvenSettlements_Knight_Dark>20</FRealms_ElvenSettlements_Knight_Dark>
                </guards>
            </li>
            <li>
                <kindDef>Settlement</kindDef>
                <options>
                    <FRealms_ElvenSettlements_Peasant_Dark>20</FRealms_ElvenSettlements_Peasant_Dark>
                    <FRealms_ElvenSettlements_Squire_Dark>20</FRealms_ElvenSettlements_Squire_Dark>
                    <FRealms_ElvenSettlements_Footsoldier_Dark>20</FRealms_ElvenSettlements_Footsoldier_Dark>
                    <FRealms_ElvenSettlements_Bowman_Dark>50</FRealms_ElvenSettlements_Bowman_Dark>
                    <FRealms_ElvenSettlements_Knight_Dark>30</FRealms_ElvenSettlements_Knight_Dark>
                    <FRealms_ElvenSettlements_Lord_Dark>50</FRealms_ElvenSettlements_Lord_Dark>
                </options>
            </li>
        </pawnGroupMakers>
    </FactionDef>

</Defs>